pipeline {
    agent any
   tools {
        maven 'Maven_3.5.2' 
    }
    stages {
        stage('Build') {
            steps {
                 bat 'clean package'
            }
            post{
                success{
                    echo 'Now archiving...'
                    archiveartifacts artifacts: '**/target/*.war'
                }
            }

        }

        }
}